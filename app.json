{
  "expo": {
    "name": "time-align-mobile",
    "slug": "time-align-mobile",
    "privacy": "public",
    "entryPoint": "./app/index.js",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.64",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesIcloudStorage": true
    },
    "android": {
      "package": "com.goodsoftwareengineering.timealignmobile",
      "versionCode": 1,
      "permissions": []
    }
  }
}

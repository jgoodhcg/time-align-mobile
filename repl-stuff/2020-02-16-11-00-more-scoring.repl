(defn where-score-the-day
  [buckets]
  (let [scores        (->> buckets (select [sp/MAP-VALS :score :where]))
        average-score (-> (reduce + scores)
                          (/ (count scores)))]

    (->> buckets (transform [:score] #(merge % {:where average-score})))))


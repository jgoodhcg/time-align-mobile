time-align-mobile.handlers> (defn relative-difference-score
  "Gives a score 0 - 100
  100 - highest score (also 100 if x-ref is 0)
  0 - lowest score"
  [x x-ref]
  (let [actual-difference (-> x-ref
                              (- x)
                              (js/Math.abs))
        denominator         (-> x-ref
                                (+ x)
                                (/ 2))
        relative-difference (-> actual-difference
                                (/ denominator))
        score               (if (= 0 x-ref)
                              0
                              relative-difference)
        inverted            (-> (- 2 score)
                                (/ 2)
                                (* 100)
                                (js/Math.round))]
    score))

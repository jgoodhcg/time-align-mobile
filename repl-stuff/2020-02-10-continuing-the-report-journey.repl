                        ;;                           bucket-id-b [periods]}}}
                        (->> (transform 
                              [sp/MAP-VALS] 
                              (fn [periods] 
                                (->> periods
                                     (group-by 
                                      (fn [period] (if (:planned periods)
                                                     :planned
                                                     :actual)))
                                     (transform [sp/MAP-VALS]
                                                (fn [periods]
                                                  (->> periods
                                                       (group-by :bucket-id))))))))
